@import url('http://fonts.googleapis.com/css?family=Roboto\+Condensed:400italic,700italic,400,300')
  
*
  box-sizing border-box
  

html
  font-size 20px
  
html, body
  height 100%
  
body
  font-family roboto condensed
  line-height 1.5
  position relative
  
.hamburger
  color white
  display block
  font-size 3rem
  position fixed
  top 1rem
  right 2rem
  z-index 1
  
  &:hover
    color orange
    cursor pointer
    
.logo
  //background-color white
  background url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/t-102/CB-150-logo.gif) no-repeat 
  display block
  position absolute
  top 2rem
  left 2rem
  height 145px
  width 185px
  z-index 1
  
.twitter
  background url('data:image/svg+xml;base64,') no-repeat 
  display block
  position absolute
  top 2.5rem
  right 8rem
  height 2rem
  width 2rem
  z-index 1
  
.facebook
  //background-color white
  background url('data:image/svg+xml;base64,') no-repeat 
  display block
  position absolute
  top 2.5rem
  right 12rem
  height 2rem
  width 2rem
  z-index 1

[class*=date]
  background url('data:image/svg+xml;base64,') no-repeat center top
  //background-size 2rem 2rem
  display inline-block
  font-weight bold
  
.date-center  
  height 2rem
  width 5rem
  margin-bottom 2rem
  padding-top 2.5rem
  text-align center
  
.date-left  
  height 2.5rem
  width 10rem
  margin-bottom 2rem
  padding-top .25rem
  text-align left
  
.date-right  
  height 2.5rem
  width 10rem
  margin-bottom 2rem
  padding-top .25rem
  text-align right
  
.row
  display table
  width 100%
  
.col
  display table-cell
  padding 2rem
  width 50%
  vertical-align middle
  
img
  height auto
  width 100%
      
[class^='slide--']
  background-position center center
  background-attachment: fixed
  background-size cover
  //box-shadow inset 0 3px 10px rgba(#333, .8)
  display table
  position: relative
  min-height 100%
  width 100%
  
[class*='text-link']
  display inline-block
  text-decoration underline
  
[class*=content]
  color white
  
.slide
  
  &__cell
    display table-cell
    //min-height 100%
    //width 100%
    vertical-align middle
  
  &__content--center
    color white
    text-align center
    margin 0 auto
    width 60%
    
  &__content--full
    margin 0
    text-align center
    width 100%

  &__headline
    font-size 3.25rem
    margin 0 2rem 1.4rem 2rem
    
  &__heading
    font-size 2rem
    
  &__base-text
    //text-align center
  
  &__button-video
    display block
    
  &__button-scroll
    display block
    
[data-slide='1']
  background-image url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/t-102/comm-slide1.jpg)
  
  
[data-slide='2']
  background-image url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/t-102/comm-slide2.jpg)
  
  &::after
    content: ' '
    position: absolute
    top: 0
    left: 49.9%
    //border-left: 2px solid #fff
    z-index: 2
    height: 100%
    
  .slide__content
    transform translateY(-2rem)
  
[data-slide='3']
  background-image url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/t-102/comm-slide3.jpg)
  
  &::after
    content: ' '
    position: absolute
    top: 0
    left: 49.9%
    border-left: 2px solid #fff
    z-index: 2
    height: 100%
    
  .slide__heading
    text-align left
    
  .slide__media
    margin 0 auto
    width 50%
  
  
[data-slide='4']
  background-image url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/t-102/comm-slide4.jpg)
  
  &::after
    content: ' '
    position: absolute
    top: 0
    left: 49.9%
    border-left: 2px solid #fff
    z-index: 2
    height: 100%
    
  .slide__heading
    text-align right
  
[data-slide='footer']
  background #267e64